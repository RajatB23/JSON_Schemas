{
  "$id": "https://github.com/RajatB23/JSON_Schemas/blob/master/localeMapEntry.schema.json",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "object",
  "description": "Locale mapping between two locales",
  "examples": [
    {
      "from": "fr-FR",
      "to": "fr-CA"
    }
  ],
  "required": ["from", "to"],
  "title": "Locale map entry",
  "properties": {
    "from": {
      "description": "From locale in mapping",
      "$ref": "locale.schema.json"
    },
    "to": {
      "description": "To locale in mapping",
      "$ref": "locale.schema.json"
    }
  },
  "additionalProperties": false
}
